            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/rnx2rtkp.exe",
            "args": ["-x","5","-p","0","-m","15","-n","-o","${workspaceFolder}\\results\\out.pos","${workspaceFolder}\\dataset\\obs\\chti${config:epoch}0.23o","${workspaceFolder}\\dataset\\eph\\BRDM00DLR_S_2023${config:epoch}0000_01D_MN.rnx"],
